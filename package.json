{
  "name": "hackernews-backend",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon ./bin/www",
    "worker": "node worker.js",
    "worker-nodemon": "nodemon worker.js",
    "test": "jest --forceExit --detectOpenHandles --passWithNoTests",
    "test:coverage": "jest --coverage --forceExit --detectOpenHandles",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "import": "node scripts/import-json-to-sqlite.js"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "better-sqlite3": "^12.6.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "debug": "~4.4.3",
    "dotenv": "^17.3.1",
    "express": "~5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "~1.10.1"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2"
  }
}
